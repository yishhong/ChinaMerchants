source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'

target 'ChinaMerchants' do
    #友盟分析 包含IDFA版本
    pod 'UMengAnalytics'
    #U-Share SDK UI模块
    pod 'UMengUShare/UI'
    # 集成新浪微博
    pod 'UMengUShare/Social/Sina'
    # 集成微信
    pod 'UMengUShare/Social/WeChat'
    # 集成QQ
    pod 'UMengUShare/Social/QQ'
    #下拉上拉刷新
    pod 'MJRefresh'
    #百度地图
    pod 'BaiduMapKit'
    #图片下载管理
    pod 'SDWebImage'
    #网络加载
    pod 'AFNetworking'
    #JSON转换Model库
    pod 'YYModel'
    #代码自动布局
    pod 'Masonry'
    #弹出框
    pod 'CNPPopupController'
    #空数据页面显示
    pod 'DZNEmptyDataSet'
    #LevelDB数据库
    pod 'Objective-LevelDB'
    #滑动banner
    pod 'SDCycleScrollView','~> 1.64'
    #键盘事件
    pod 'IQKeyboardManager'
    #极光推送
    pod 'JPush'
    # 方便调试的辅助插件
    pod 'FLEX'
    # 集成环信
    pod 'Hyphenate'
    #集成本地环信UI
    pod 'EaseUI', :path => 'EaseUI'

end
